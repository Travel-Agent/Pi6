{
  "name": "Shucts",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Add keyboard-shortcuts to google-search!",
  "homepage_url": "https://github.com/wildeyes/shucts",
  "options_page": "src/options.html",
  "background": {
    "scripts":["src/eventpage.js"],
    "persistent":false
  },
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  "content_scripts": [
    {
      "matches": [
        "*://www.google.com/*"
      ],
      "js": [
        "lib/jquery.js",
        "lib/keyboard.js",
        "src/inject.js"
      ]
    }
  ]
}