{
  "name": "qList",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "Add keyboard-shortcuts to google-search!",
  "homepage_url": "https://github.com/wildeyes/qList",
  /*"options_page": "src/options.html",*/
  "background": {
    "scripts":["src/eventpage.js"],
    "persistent":false
  },
  "icons": { "16": "icon16.png",
           "48": "icon48.png",
          "128": "icon128.png" },
  "content_scripts": [
    {
      "matches": [
        "*://www.google.com/*",
        "*://www.google.co.il/*"
      ],
      "js": [
        "lib/mousetrap.js",
        "src/script.js"
      ]
    }
  ]
}