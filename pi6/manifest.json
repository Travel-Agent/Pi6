{
    "name"                     : "Pi6"
   ,"version"                  : "2.22.0"
   ,"manifest_version"         : 2
   ,"is_dev"                   : true
   ,"description"              : "Search and lists keyboard shortcuts for the ADHD (Chrome Extension)"
   ,"homepage_url"             : "https://github.com/wildeyes/Pi6"
   ,"update_url"               : "https://raw.github.com/wildeyes/Pi6/master/data/updates.xml"
   ,"background"               : {
        "scripts"              : ["jquery.js","eventpage.js"]
       ,"persistent"           : true
    }
   ,"content_scripts"          : [{
        "matches"              : [
            "<all_urls>"
        ]
       ,"js"                   : ["underscore.js","jquery.js", "mousetrap.js", "script.js"]
    }]
   ,"permissions"              : ["storage","https://raw.github.com/wildeyes/Pi6/master/data/rocket.min.json"]
   ,"web_accessible_resources" : ["test-rocket.min.js"]
}