{
    "name": "Pi6"
   ,"version": "2.2.0"
   ,"manifest_version": 2
   ,"description": "Search and lists keyboard shortcuts for the ADHD"
   ,"homepage_url": "https://github.com/wildeyes/Pi6"
   ,"update_url": "https://raw.github.com/wildeyes/Pi6/master/data/updates.xml"
   ,"background": {
        "scripts": ["jquery.js","eventpage.js"]
       ,"persistent": false
    }
   ,"content_scripts": [{
        "matches": [
            "<all_urls>"
        ]
       ,"js": ["jquery.js", "mousetrap.js", "script.js"]
    }]
   ,"permissions": ["storage", "https://raw.github.com/wildeyes/Pi6/master/data/rocket.min.json"]
}