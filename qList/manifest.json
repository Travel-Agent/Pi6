
{
  "name"            : "qList",
  "version"         : "2.0.2",
  "manifest_version": 2,
  "description"     : "Awesome Search Keyboard Shortcuts",
  "homepage_url"    : "https://github.com/wildeyes/qList",
  "update_url"      : "https://github.com/wildeyes/qList",
  "background": {
    "scripts"   :["eventpage.js"],
    "persistent":false
  },
  "icons": {
           "16" : "icon16.png",
           "48" : "icon48.png",
           "128": "icon128.png"
          },
  "content_scripts": [
    {
      "matches": [
        "*://www.google.com/*"
      , "*://thepiratebay.sx/search/*"
      , "*://www.google.co.il/*"
      , "*://israblog.nana10.co.il/*"
      , "*://www.youtube.com/results?*"
      , "*://www.cheatography.com/*"
      , "*://readthedocs.org/*"
      ],"js": [
        "mousetrap.js",
        "jquery.js",
        "data.js",
        "script.js"
      ]
    }
  ]
}
